/**index.wxss**/

page {
    background-color: #f5f5f5;
}

.container {
    position: fixed;
    width: 100%;
    top: 0;
    left: 0;
}

@media (prefers-color-scheme: dark) {
    page {
        background-color: #111;
    }
}

.prev {
    color: red !important;
}

.next {
    color: green;
}

.wx-calendar-panel-date.prev {
    color: red;
}

.wx-calendar-panel-date {
    color: red;
}


.curr > .wx-calendar-panel-day {
    color: white !important;
}

